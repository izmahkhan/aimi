<IfModule mod_rewrite.c>
#Options +FollowSymlinks
RewriteEngine On
ErrorDocument 404 https://aimi.edu.pk/page-not-found
RewriteCond %{HTTPS} off
# First rewrite to HTTPS:
# Don't put www. here. If it is already there it will be included, if not
# the subsequent rule will catch it.
RewriteRule .* https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]


RewriteRule ^([^./\\"'?#]+)$ index.php?page=$1 [L,QSA]	

RewriteRule ^/?(merit-list/mbbs)$  index.php?page=merit-list-detail&type=mbbs&by=name [L,QSA]
RewriteRule ^/?(merit-list/bds)$  index.php?page=merit-list-detail&type=bds&by=name [L,QSA]

RewriteRule ^/?(merit-list/mbbs)/([^./\\"'?#]+)$  index.php?page=merit-list-detail&type=mbbs&list=$2 [L,QSA]
RewriteRule ^/?(merit-list/bds)/([^./\\"'?#]+)$  index.php?page=merit-list-detail&type=bds&list=$2 [L,QSA]

RewriteRule ^/?(list-temp/mbbs)/([^./\\"'?#]+)$  index2.php?page=list-temp&type=mbbs&list=$2 [L,QSA]
RewriteRule ^/?(list-temp/bds)/([^./\\"'?#]+)$  index2.php?page=list-temp&type=bds&list=$2 [L,QSA]

RewriteRule ^/?(merit-list/mbbs-agg)$  index.php?page=merit-list-detail&type=mbbs&by=agg [L,QSA]
RewriteRule ^/?(merit-list/bds-agg)$  index.php?page=merit-list-detail&type=bds&by=agg [L,QSA]

RewriteRule ^/?(waiting-list-mbbs)/([^./\\"'?#]+)$  index.php?page=waiting-list-mbbs-detail&id=$2 [L,QSA]
RewriteRule ^/?(waiting-list-bds)/([^./\\"'?#]+)$  index.php?page=waiting-list-bds-detail&id=$2 [L,QSA]





RewriteRule ^/?(news)/([^./\\"'?#]+)$  index.php?page=news_detail&slug=$2 [L,QSA]
RewriteRule ^/?(event)/([^./\\"'?#]+)$  index.php?page=news_event&event=$2 [L,QSA]
RewriteRule ^/?(category)/([^./\\"'?#]+)$  index.php?page=news_categories&category=$2 [L,QSA]
RewriteRule ^/?(search_news/)/([^./\\"'?#]+)$  index.php?page=news_search?search_inp [L,QSA]
RewriteRule ^/?(download_application_form)/$  index.php?page=$1&ja_id=$2 [L,QSA]
RewriteRule ^/?(download_application_form)/([^./\\"'?#]+)$  index.php?page=$1&ja_id=$2 [L,QSA]



</IfModule>

<IfModule mod_expires.c>
# Enable expirations
ExpiresActive On
# My favicon
ExpiresByType image/x-icon "access plus 1 year"
# Images
ExpiresByType image/gif "access plus 1 month"
ExpiresByType image/png "access plus 1 month"
ExpiresByType image/jpg "access plus 1 month"
ExpiresByType image/jpeg "access plus 1 month"

# Javascript
ExpiresByType application/javascript "access plus 1 month"

# CSS
ExpiresByType text/css "access plus 1 month"

# HTML
ExpiresByType text/html "access plus 2 days"

</IfModule>

# php -- BEGIN cPanel-generated handler, do not edit
# Set the “ea-php74” package as the default “PHP” programming language.
<IfModule mime_module>
  AddHandler application/x-httpd-ea-php74 .php .php7 .phtml
</IfModule>
# php -- END cPanel-generated handler, do not edit
